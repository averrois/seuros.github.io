{
  "name": "seuros-blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx astro dev",
    "start": "bunx astro dev",
    "build": "bunx astro check && bunx astro build",
    "preview": "bunx astro preview",
    "astro": "bunx astro",
    "lint": "eslint . --ext .js,.ts,.astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,astro}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.2.6",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.3.1",
    "@astrojs/tailwind": "^5.1.5",
    "astro": "^5.7.12",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.32.0"
  }
}
